  "main": "server.js",
  "scripts": {
    "type": "module",
    "server": "nodemon backend/src/server.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:single": "sequelize db:migrate --to",
    "db:seed": "sequelize db:seed:all",
    "db:seed:single": "sequelize db:seed --seed",
    "db:undo": "sequelize db:migrate:undo",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all"
  },
